@using Application.ViewModels.ConfiguracionRetorno;

@model DeleteConfiguracionRetornoViewModels

@{
    ViewData["Title"] = "Eliminar Tasa de Retorno";
}


<div class="col-12  col-md-6 offset-md-3">
    <div class="card">
        <div class="card-hearder bg-success  text-ligh ">
            <h4 class="text-center"> </h4>
            <h5 class="text-center">Estas seguro que deseas eliminar esta Tasa de Retorno?</h5>
  

        </div>
        <div class="card-body">
            <form asp-controller="ConfiguracionRetorno" asp-action="Delete" method="POST">
                <input type="hidden" asp-for="Id" />


                <div>
                    <button type="submit" class="btn btn-outline-danger float-end">Eliminar</button>
                    <a asp-controller="ConfiguracionRetorno" asp-action="Index" class="btn btn-outline-warning me-2">Regresar</a>
                </div>
            </form>
        </div>
    </div>
</div>
